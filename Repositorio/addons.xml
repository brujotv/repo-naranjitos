<addons>
<addon id="plugin.video.CatoalWizard"
	   name="Catoal[COLOR blue] WI[/COLOR][COLOR yellow] ZA[/COLOR][COLOR red] RD[/COLOR]"
	   version="1.0.2"
	   provider-name="Catoal, RonnieHD">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Catoal Wizard</summary>
    <description>Instalador inteligente que ayuda a configurar tu Kodi correctamente</description>
